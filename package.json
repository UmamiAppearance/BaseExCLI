{
  "name": "base-ex-cli",
  "version": "0.1.0",
  "description": "Command Line Interface for BaseEx",
  "main": "./cjs/index.cjs",
  "module": "./index.js",
  "bin": {
    "bx": "./index.js"
  },
  "exports": {
    "import": "./index.js",
    "require": "./cjs/index.cjs"
  },
  "type": "module",
  "directories": {
    "cjs": "cjs"
  },
  "scripts": {
    "build:cjs": "rollup .index.js --file cjs/index.cjs --format cjs",
    "lint": "eslint ./index.js",
    "lint:fix": "eslint ./index.js --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UmamiAppearance/BaseExCLI.git"
  },
  "keywords": [
    "CLI",
    "Command Line Interface",
    "BaseEx",
    "base-ex"
  ],
  "author": "UmamiAppearance",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/UmamiAppearance/BaseExCLI/issues",
    "email": "mail@umamiappearance.eu"
  },
  "homepage": "https://github.com/UmamiAppearance/BaseExCLI#readme",
  "devDependencies": {
    "eslint": "^8.24.0",
    "rollup": "^2.79.1",
  },
  "Dependencies": {
    "base-ex": "^0.5.0",
    "yargs": "^17.6.0"
  }
}
